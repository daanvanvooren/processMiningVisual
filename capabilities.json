        },
        {
            "name": "duration",
            "kind": "GroupingOrMeasure",
            "displayName": "Duration"
        }
    ],
    "objects": {
        "graphSettings": {
            "displayName": "Graphsettings",
            "properties": {
                "relationShipPercentageThreshold": {
