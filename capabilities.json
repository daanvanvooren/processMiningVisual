                            }
                        },
                        {
                            "for": {
                                "in": "toActivity"
                            }
                        },
                        {
                            "for": {
                                "in": "isHappyPath"
                            }
                        },
                        {
                            "for": {
                                "in": "duration"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 30000
                        }
                    }
                }
            }
        }
    ]
